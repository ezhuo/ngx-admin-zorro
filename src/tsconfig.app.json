{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "types": [],
    "paths": {
        "@shared/*": [ "app/shared/*" ],
        "@core/*": [ "app/core/*" ],
        "@helpers": [ "app/helpers/"],
        "@helpers/*": [ "app/helpers/*"],
        "@routes/*": [ "app/routes/*" ],
        "@theme/*": [ "app/theme/*" ],
        "@testing/*": [ "testing/*" ],
        "@env/*": [ "environments/*" ],
        "@delon/*": [ "./delon/*" ]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts",
    "./delon/cli/**/*"
  ]
}
